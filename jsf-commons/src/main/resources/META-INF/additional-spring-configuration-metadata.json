{
  "properties": [
    {
      "name": "spring.task.corePoolSize",
      "type": "java.lang.Integer",
      "description": "线程池创建时候初始化的线程数."
    },
    {
      "name": "spring.task.maxPoolSize",
      "type": "java.lang.Integer",
      "description": "线程池最大的线程数，只有在缓冲队列满了之后才会申请超过核心线程数的线程."
    },
    {
      "name": "spring.task.queueCapacity",
      "type": "java.lang.Integer",
      "description": "用来缓冲执行任务的队列."
    },
    {
      "name": "spring.task.keepAliveSeconds",
      "type": "java.lang.Integer",
      "description": "当超过了核心线程出之外的线程在空闲时间到达之后会被销毁."
    },
    {
      "name": "spring.task.waitOnShutdown",
      "type": "java.lang.Boolean",
      "description": "设置线程池关闭的时候等待所有任务都完成再继续销毁其他的Bean."
    },
    {
      "name": "spring.task.awaitTerminationSeconds",
      "type": "java.lang.Integer",
      "description": "设置线程池中任务的等待时间，如果超过这个时候还没有销毁就强制销毁."
    },
    {
